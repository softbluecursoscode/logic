início {
    // Mostre os números PARES do intervalo entre MIN e MAX
    
    inteiro: min;
    inteiro: max;
    inteiro: n;
    
    leia("Mínimo", min);
    leia("Máximo", max);
    
    n = min;
    enquanto(n <= max) {
        se (n % 2 == 0) {
            escreva(n);
        }
        
        n = n + 1;
    }
}